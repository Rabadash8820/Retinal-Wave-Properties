<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MeaData" namespace="MeaData" auto-import="false">
  <class name="Project" table="projects" dynamic-update="true">
    <id name="Guid" column="Id">
      <generator class="guid"/>
    </id>
    <natural-id mutable="true">
      <property name="Name" column="Name" not-null="true"/>
      <property name="DateStarted" column="DateStarted" not-null="true"/>
    </natural-id>
    <property name="Comments" column="Comments" not-null="false" />
    <set name="Populations" table="project_populations" lazy="extra" access="nosetter.camelcase-underscore" inverse="true" cascade="save-update">
      <key unique="true" column="ProjectId" not-null="true"/>
      <many-to-many class="Population" column="PopulationId" />
    </set>
  </class>
</hibernate-mapping>