<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="NeuroData" namespace="Neuro" auto-import="false">
  <class name="ProjectRecording" table="ProjectRecordings" dynamic-update="true">
    <id name="Guid" column="Id">
      <generator class="guid"/>
    </id>
    <natural-id mutable="true">
      <many-to-one name="Project" column="ProjectId" not-null="true"/>
      <many-to-one name="Recording" column="RecordingId" not-null="true"/>
    </natural-id>
    <property name="IsOriginal" column="IsOriginal" not-null="false" />
  </class>
</hibernate-mapping>