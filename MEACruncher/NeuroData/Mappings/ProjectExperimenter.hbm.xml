<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MeaData" namespace="MeaData" auto-import="false">
  <class name="ProjectExperimenter" table="project_experimenters" dynamic-update="true">
    <id name="Guid" column="Id">
      <generator class="guid"/>
    </id>
    <natural-id mutable="true">
      <many-to-one name="Project" column="ProjectId" not-null="true"/>
      <many-to-one name="Experimenter" column="ExperimenterId" not-null="true"/>
      <property name="StartDate" column="StartDate" not-null="false"/>
    </natural-id>
    <property name="EndDate" column="EndDate" not-null="false"/>
    <property name="IsManager" column="IsManager" not-null="false" length="25"/>
  </class>
</hibernate-mapping>