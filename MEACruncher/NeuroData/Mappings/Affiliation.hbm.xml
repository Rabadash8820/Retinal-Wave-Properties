<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MeaData" namespace="MeaData" auto-import="false">
  <class name="Affiliation" table="affiliations" dynamic-update="true">
    <id name="Guid" column="Id">
      <generator class="guid"/>
    </id>
    <natural-id mutable="true">
      <many-to-one name="Experimenter" column="ExperimenterId" not-null="true"/>
      <many-to-one name="Organization" column="OrganizationId" not-null="true"/>
      <many-to-one name="Role" column="RoleId" not-null="true"/>
      <property name="StartDate" column="StartDate" not-null="true"/>
    </natural-id>
    <property name="EndDate" column="EndDate" not-null="false"/>
  </class>
</hibernate-mapping>